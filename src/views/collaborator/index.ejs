<!DOCTYPE html>
<html>
<div class="container">
            
    <div class="modal fade" id="myModal" role="dialog">
      <div class="modal-dialog">
      
        <div class="modal-content">
          <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal">&times;</button>
            
          </div>
          <div class="modal-body">
            <h4 class="modal-title">Choose Collaborators</h4>
              <table class="table table-bordered table-hover table-index-wikis">
                  <thead>
                      <tr>
                        <th class="table-wikis-head">Users</th>
                        <th class="table-wikis-head">by user</th>
                      </tr>
                    </thead>
                    <tbody>
                      <% users.forEach((user) => { %>
                        <tr>
                          <td><%= user.email %></td>
                          <td>
                              <input type="checkbox" name="user.id" value="user.id">
                          </td>
                        </tr>
                      <% }) %>
                    </tbody>
              </table>
          </div>
          <div class="modal-footer">
            <input type="submit" value="user.id" name="userId" class="btn btn-default">
            <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
          </div>
        </div>
        
      </div>
    </div>
    
  </div>
  </html>